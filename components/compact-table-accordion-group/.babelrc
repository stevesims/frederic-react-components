{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    ["@babel/preset-stage-1", { "decoratorsLegacy": true }]
  ],
  "plugins": [
    ["transform-react-remove-prop-types", { "mode": "wrap" }],
    [
      "@babel/plugin-transform-runtime",
      { "helpers": false, "polyfill": false, "regenerator": true }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    }
  }
}
