{
  "name": "@govuk-frederic/distraction-free",
  "version": "0.1.0",
  "dependencies": {
    "@govuk-frederic/arrow-left": "^0.1.0",
    "@govuk-react/constants": "^0.2.7",
    "govuk-colours": "^1.0.3"
  },
  "devDependencies": {
    "@govuk-react/storybook-components": "^0.2.4",
    "@storybook/addon-knobs": "^3.4.2"
  },
  "peerDependencies": {
    "emotion": ">=9",
    "prop-types": ">=15",
    "react": ">=16.2.0",
    "react-emotion": ">=9"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:es",
