{
  "name": "@govuk-frederic/table",
  "version": "0.2.0",
  "dependencies": {
    "@govuk-react/constants": "^0.6.0-alpha.1",
    "@govuk-react/hoc": "^0.6.0-alpha.1"
  },
  "peerDependencies": {
    "react": ">=16.2.0",
    "styled-components": ">=4"
  },
  "scripts": {
    "build": "yarn build:lib && yarn build:es",
    "build:lib": "rimraf lib && babel src -d lib --source-maps --config-file ../../babel.config.js",
    "build:es": "rimraf es && cross-env BABEL_ENV=es babel src -d es --source-maps --config-file ../../babel.config.js",
    "docs": "doc-component ./lib/index.js ./README.md"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "author": "Steve Sims",
  "contributors": [
    "Taran Chauhan",
    "Gavin Orland"
  ],
  "license": "MIT",
  "homepage": "https://github.com/stevesims/govuk-frederic/tree/master/components/table",
  "description": "Renders a table with optional heading row. Cell data is derived from an array and can include names per cell or per row.",
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
